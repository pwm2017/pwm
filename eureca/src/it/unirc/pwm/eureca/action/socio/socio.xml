<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="socio" namespace="/" extends="SecureAdministrator">

		<action name="InserisciSocioRedirect">
 			<interceptor-ref name="authenticationAdministratorInterceptor" /> 
			<result>/WEB-INF/areaRiservata/amministratore/AggiungiSocio.jsp </result>
		</action>


		<action name="AggiungiSocio" class="it.unirc.pwm.eureca.action.socio.AggiungiSocio">
			<interceptor-ref name="defaultStack" />
			<interceptor-ref name="timer" />
			<interceptor-ref name="logger" />
			<interceptor-ref name="authenticationAdministratorInterceptor" />
			<result name="success">/WEB-INF/areaRiservata/amministratore/indexAmministratore.jsp
			</result>
			<result name="input">/HOME.action</result>
		</action>

	</package>

</struts>
